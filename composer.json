{
    "name": "tbrouard/paginator",
    "type": "library",
    "description": "A lightweight PHP paginator, for generating pagination controls in the style of Stack Overflow and Flickr. The 'first' and 'last' page links are shown inline as page numbers, and excess page numbers are replaced by ellipses.",
    "keywords": ["paginator", "pagination", "pager"],
    "homepage": "https://github.com/Sevengroup-IT/php-paginator",
    "license": "MIT",
    "authors": [
        {
            "name": "Jason Grimes",
            "email": "jason@grimesit.com"
        }
    ],
    "developpers": [
        {
            "name": "Thierry Brouard",
            "email": "thierry.brouard@sevengroup.fr"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.2"
    },
    "autoload": {
        "psr-0": {"Sevengroup": "src/"}
    },
    "autoload-dev": {
        "psr-0": {"Sevengroup\\Tests": "tests/"}
    }

}


